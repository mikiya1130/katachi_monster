{
  "name": "frontend",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start --port 3030",
    "lint": "next lint --dir src",
    "lint:fix": "next lint --dir src --fix",
    "format": "yarn update-react-imports && yarn lint:fix && prettier --write src",
    "update-react-imports": "echo yes | npx react-codemod update-react-imports src --force --parser tsx"
  },
  "dependencies": {
    "@emotion/react": "^11.11.1",
    "@emotion/styled": "^11.11.0",
    "@fontsource/noto-sans-jp": "^5.0.14",
    "@fontsource/roboto": "^5.0.8",
    "@material-ui/core": "^4.12.4",
    "@mui/icons-material": "^5.14.13",
    "@mui/material": "^5.14.13",
    "next": "13.5.4",
    "react": "^18",
    "react-dom": "^18",
    "react-swipeable": "^7.0.1"
  },
  "devDependencies": {
    "@types/node": "^20",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "@typescript-eslint/eslint-plugin": "^6.7.5",
    "eslint": "^8",
    "eslint-config-next": "13.5.4",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-no-relative-import-paths": "^1.5.2",
    "eslint-plugin-unused-imports": "^3.0.0",
    "prettier": "^3.0.3",
    "typescript": "^5"
  },
  "peerDependencies": {
    "react": "^17.0.0 || ^18.0.0",
    "react-dom": "^17.0.0 || ^18.0.0"
  }
}
